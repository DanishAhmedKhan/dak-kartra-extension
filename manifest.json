{
    "manifest_version": 2,
    "name": "Kartra extension",
    "version": "0.1",
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        "storage"
    ],
    "web_accessible_resources": [
        "images/*.jpg"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery.js", "content.js"]
        }
    ]
}